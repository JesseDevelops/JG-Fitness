@{
    ViewData["Title"] = "Five Days A Week";
}
    <section class="main-container-workoutlist">
        <article class="workoutFive-intro">
            <div class="jumbotron">
                <div class="container">
                    <div class="row">
                        <div class="col-sm-10">
                            <h1>Back Workout</h1>
                            <ul>
                                <li><b>Deadlift:</b> 4 sets x 6 reps</li>
                                <li><b>Dumbbell Row:</b> 4 sets x 8-12 reps</li>
                                <li><b>Hammer Strength Machine Row:</b> 4 sets x 8-12 reps</li>
                                <li><b>Lat Pull Down:</b> 4 sets x 8-12 reps</li>
                                <li><b>One Arm Cable Row:</b> 3 sets x 12 reps</li>
                                <li><b>Straight Arm Cable Pull Down:</b> 2 sets x 15reps</li>
                            </ul>
                        </div>
                        <div class="col-sm-2"></div>
                    </div>
                </div>
            </div>
        </article>



        <article class="workoutFive-middleUpper">
            <div class="jumbotron">
                <div class="container">
                    <div class="row">
                        <div class="col-sm-10">
                            <h1>Chest & Abs Workout</h1>
                            <ul>
                                <li><b>Incline Bench Press:</b> 4 sets x 6 reps</li>
                                <li><b>Decline Bench Press:</b> 4 sets x 8-12 reps</li>
                                <li><b>Machine Bench Press:</b> 3 sets x 6-12 reps</li>
                                <li><b>Machine Fly:</b> 3 sets x 12-15 reps</li>
                                <li><b>Machine Crunch:</b> 3 sets x 15 reps</li>
                                <li><b>Hanging Leg Raise:</b> 3 sets x 12-15 reps</li>
                            </ul>
                        </div>
                        <div class="col-sm-2"></div>
                    </div>
                </div>
            </div>
        </article>

        <article class="workoutFive-middleInner">
            <div class="jumbotron">
                <div class="container">
                    <div class="row">
                        <div class="col-sm-10">
                            <h1>Legs Workout</h1>
                            <ul>
                                <li><b>Barbell Back Squat:</b> 5 sets x 6 reps</li>
                                <li><b>Romanian Deadlift:</b> 4 sets x 8-12 reps</li>
                                <li><b>Leg Press:</b> 3 sets x 10-15 reps</li>
                                <li><b>Leg Curl:</b> 3 sets x 12-15 reps</li>
                                <li><b>Walking Lunge:</b> 3 sets x 15 reps each leg</li>
                                <li><b>Smith Machine Calf Raise:</b> 4 sets x 20 reps</li>
                            </ul>
                        </div>
                        <div class="col-sm-2"></div>

                    </div>
                </div>
            </div>
        </article>

        <article class="workoutFive-middleEnd">
            <div class="jumbotron">
                <div class="container">
                    <div class="row">
                        <div class="col-sm-10">
                            <h1>Shoulders & Abs Workout</h1>
                            <ul>
                                <li><b>Military Press:</b> 4 sets x 6 reps</li>
                                <li><b>Lateral Raise:</b> 4 sets x 8-12 reps</li>
                                <li><b>Reverse Machine Fly:</b> 4 sets x 8-12 reps</li>
                                <li><b>Machine Shoulder Press:</b> 3 sets x 6-10 reps</li>
                                <li><b>Barbell Shrugs:</b> 4 sets x 15 reps</li>
                                <li><b>Decline Sit Up:</b> 3 sets x 15 reps</li>
                                <li><b>Barbell Roll Out:</b> 3 sets x 12 reps</li>
                            </ul>
                        </div>
                        <div class="col-sm-2"></div>
                    </div>
                </div>
            </div>
        </article>

        <article class="workoutFive-end">
            <div class="jumbotron">
                <div class="container">
                    <div class="row">
                        <div class="col-sm-10">
                            <h1>Arms Workout</h1>
                            <ul>
                                <li><b>EZ Bar Curl:</b> 4 sets x 8-12 reps</li>
                                <li><b>EZ Bar Skullcrusher:</b> 4 sets x 8-12 reps</li>
                                <li><b>Spider Curl:</b> 4 sets x 8-12 reps</li>
                                <li><b>Tricep Dip:</b> 4 sets x 8-12 reps</li>
                                <li><b>Cable Hammer Curl:</b> 4 sets x 8-12 reps</li>
                                <li><b>Cable Overhead Tricep Extension:</b> 4 sets x 8-12 reps</li>
                            </ul>
                        </div>
                        <div class="col-sm-2"></div>
                    </div>
                </div>
            </div>
        </article>



    </section>