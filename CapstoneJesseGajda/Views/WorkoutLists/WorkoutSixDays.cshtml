@{
    ViewData["Title"] = "Five Days A Week";
}
    <section class="main-container-workoutlist">
        <article class="workoutSix-intro">
            <div class="jumbotron">
                <div class="container">
                    <div class="row">
                        <div class="col-sm-10">
                            <h1>Leg Workout</h1>
                            <ul>
                                <li><b>Lying Dumbbell Decline Leg Curl:</b> 3 sets x 6-10 reps</li>
                                <li><b>Romanian Deadlift:</b> 3 sets x 6-8 paused reps</li>
                                <li><b>Dumbbell Walking Lunges:</b> 3 sets x 12 reps each leg</li>
                                <li><b>Leg Press:</b> 4 sets x 15-20 reps</li>
                                <li><b>Leg Extension:</b> 3 sets x 12 reps</li>
                                <li><b>Standing Calves:</b> 1 sets x 10/10/10 dropset</li>
                            </ul>
                        </div>
                        <div class="col-sm-2"></div>
                    </div>
                </div>
            </div>
        </article>



        <article class="workoutSix-middleUpper">
            <div class="jumbotron">
                <div class="container">
                    <div class="row">
                        <div class="col-sm-10">
                            <h1>Chest & Arms Workout</h1>
                            <ul>
                                <li><b>Dips:</b> 4 sets x 5 reps</li>
                                <li><b>Low Incline Dumbbell Press:</b> 3 sets x 8-12 reps</li>
                                <li><b>Straight Bar Cable Curls with Decline EZ Deadstop Extension SuperSet:</b> 3 sets x 6-10 reps</li>
                                <li><b>Standing Incline supinated Dumbbell Curl with PJR Pullover:</b> 3 sets x 8-12 reps</li>
                                <li><b>Dumbbell Barbell Press:</b> 4 sets x 6 reps</li>
                            </ul>
                        </div>
                        <div class="col-sm-2"></div>
                    </div>
                </div>
            </div>
        </article>

        <article class="workoutSix-middleInner">
            <div class="jumbotron">
                <div class="container">
                    <div class="row">
                        <div class="col-sm-10">
                            <h1>Back & Shoulders</h1>
                            <ul>
                                <li><b>Bent-Over Row:</b> 4 sets x 6-10 reps</li>
                                <li><b>Pull-Ups:</b> 5 sets x 8 reps</li>
                                <li><b>Chest-Supported Row:</b> 3 sets x 10-15 reps</li>
                                <li><b>Machine Shoulder Press:</b> 3 sets x 8-12 reps</li>
                                <li><b>Cable One-Arm Lateral Raise:</b> 3 sets x 12 </li>
                                <li><b>Cable Bent-Over Laterals:</b> 4 sets x 10-15 reps</li>
                                <li><b>Machine Shrugs:</b> 4 sets x 15-20 reps</li>
                            </ul>
                        </div>
                        <div class="col-sm-2"></div>

                    </div>
                </div>
            </div>
        </article>

        <article class="workoutSix-middleInnerLower">
            <div class="jumbotron">
                <div class="container">
                    <div class="row">
                        <div class="col-sm-10">
                            <h1>Legs</h1>
                            <ul>
                                <li><b>Lying Leg Curl:</b> 3 sets x 12 reps</li>
                                <li><b>Dumbbell Romanian Deadlift:</b> 3 sets x 8-12 Paused Reps</li>
                                <li><b>Dumbbell Deficit Reverse Lunges:</b> 3 sets x 8-12 Reps each leg</li>
                                <li><b>Hack Squat:</b> 4 sets x 6-10 reps</li>
                                <li><b>Leg Extension:</b> 2 sets x 20-30 reps</li>
                            </ul>
                        </div>
                        <div class="col-sm-2"></div>
                    </div>
                </div>
            </div>
        </article>

        <article class="workoutSix-Lower">
            <div class="jumbotron">
                <div class="container">
                    <div class="row">
                        <div class="col-sm-10">
                            <h1>Chest & Arms</h1>
                            <ul>
                                <li><b>Barbell Inclines:</b> 4 sets x 8-12 reps</li>
                                <li><b>Cable Flyes:</b> 4 sets x 8-12 reps</li>
                                <li><b>Superset: Dumbbell spider curls x V Bar Pushdowns</b> 4 sets x 8-12 reps</li>
                                <li><b>Superset: Seated Incline Hammer Curls x Overhead Kneeling Cable Extensions</b> 4 sets x 8-12 reps</li>
                                <li><b>Seated chest press</b> 3 sets x 8-12 reps</li>
                            </ul>
                        </div>
                        <div class="col-sm-2"></div>
                    </div>
                </div>
            </div>
        </article>

        <article class="workoutSix-End">
            <div class="jumbotron">
                <div class="container">
                    <div class="row">
                        <div class="col-sm-10">
                            <h1>Back & Shoulders</h1>
                            <ul>
                                <li><b>Chest Supported Rows(Dumbbell or Machine):</b> 4 sets x 8-12 reps</li>
                                <li><b>Dante Rows:</b> 3 sets x 15 reps</li>
                                <li><b>Rear Delt Swings:</b> 3 sets x 25 reps</li>
                                <li><b>Arnold Press:</b> 4 sets x 8-12 reps</li>
                                <li><b>Seated Shrugs:</b> 4 sets x 15 reps</li>
                                <li><b>Dumbbell Front Press:</b> 4 sets x 8-12 reps</li>
                            </ul>
                        </div>
                        <div class="col-sm-2"></div>
                    </div>
                </div>
            </div>
        </article>



    </section>